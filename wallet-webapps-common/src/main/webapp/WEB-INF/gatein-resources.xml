<gatein-resources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.exoplatform.org/xml/ns/gatein_resources_1_4 http://www.exoplatform.org/xml/ns/gatein_resources_1_4" xmlns="http://www.exoplatform.org/xml/ns/gatein_resources_1_4">
  <portal-skin>
    <skin-name>Default</skin-name>
    <skin-module>wallet</skin-module>
    <css-path>/css/platform.css</css-path>
    <css-priority>0</css-priority>
  </portal-skin>

  <portal-skin>
    <skin-name>Enterprise</skin-name>
    <skin-module>wallet</skin-module>
    <css-path>/css/platform.css</css-path>
    <css-priority>0</css-priority>
  </portal-skin>

  <module>
    <name>walletCommon</name>
    <script>
      <minify>false</minify>
      <adapter>
        (function() {
          <include>/js/walletCommon.bundle.js</include>
          return WalletCommon;
        })();
      </adapter>
    </script>
    <depends>
      <module>vue</module>
    </depends>
    <depends>
      <module>jquery</module>
      <as>$</as>
    </depends>
    <depends>
      <module>walletJQueryUI</module>
    </depends>
    <depends>
      <module>fetchPolyfill</module>
    </depends>
    <depends>
      <module>customEventPolyfill</module>
    </depends>
    <depends>
      <module>iePolyfills</module>
    </depends>
    <depends>
      <module>vuetify</module>
    </depends>
    <depends>
      <module>Web3</module>
      <as>LocalWeb3</as>
    </depends>
    <depends>
      <module>ethQRCode</module>
      <as>EthereumQRPlugin</as>
    </depends>
    <depends>
      <module>abiDecoder</module>
    </depends>
    <depends>
      <module>userPopupPlugin</module>
    </depends>
    <depends>
      <module>spacePopupPlugin</module>
    </depends>
  </module>

  <module>
    <name>vuetify</name>
    <script>
      <minify>false</minify>
      <path>/js/lib/vuetify.min.js</path>
    </script>
    <depends>
      <module>vue</module>
    </depends>
  </module>

  <module>
    <name>abiDecoder</name>
    <script>
      <minify>false</minify>
      <adapter>
        (function() {
          <include>/js/lib/abi-decoder.js</include>
          return abiDecoder;
        })();
      </adapter>
    </script>
    <depends>
      <module>vue</module>
    </depends>
  </module>

  <module>
    <name>ethQRCode</name>
    <script>
      <minify>false</minify>
      <path>/js/lib/ethereum-qr-code.min.js</path>
    </script>
    <depends>
      <module>vue</module>
    </depends>
  </module>

  <module>
    <name>Web3</name>
    <script>
      <minify>false</minify>
      <adapter>
        (function() {
          <include>/js/lib/web3.min.js</include>
          return Web3;
        })();
      </adapter>
    </script>
  </module>

  <module>
    <name>walletJQueryUI</name>
    <as>JQueryUI</as>
    <script>
      <minify>false</minify>
      <path>/js/lib/jquery-ui-1.18.12.min.js</path>
    </script>
    <depends>
      <module>jquery</module>
      <as>jQuery</as>
    </depends>
  </module>

  <module>
    <name>vueChart</name>
    <as>VueChart</as>
    <script>
      <minify>false</minify>
      <path>/js/lib/vue-chartjs.min.js</path>
    </script>
  </module>

  <module>
    <name>chart</name>
    <as>Chart</as>
    <script>
      <minify>false</minify>
      <path>/js/lib/Chart.bundle.min.js</path>
    </script>
  </module>

</gatein-resources>