{
  "name": "exo-ethereum-wallet",
  "version": "1.3.0",
  "description": "eXo Ethereum Wallet",
  "repository": {
    "type": "git",
    "url": "https://github.com/exo-addons/exo-ethereum-wallet"
  },
  "scripts": {
    "start": "webpack --config webpack.dev.js --mode development",
    "dev": "webpack-dev-server --config webpack.dev.js --mode development --hot",
    "watch": "webpack --config webpack.watch.js --progress --colors --watch -d",
    "test": "jest --runInBand",
    "test1": "jest src/test/specs/*.test1.js --runInBand",
    "test2": "jest src/test/specs/*.test2.js --runInBand",
    "test3": "jest src/test/specs/*.test3.js --runInBand",
    "test-coverage": "jest --runInBand --coverage",
    "format": "prettier --config ./.prettierrc --write ./src/test/**/*.js ./src/main/webapp/vue-app/**.js ./src/test/**.js",
    "lint": "eslint --fix ./src/main/webapp/vue-app/*.js ./src/main/webapp/vue-app/js/*.js ./src/test/*.js ./src/test/specs/*.js ./src/main/webapp/vue-app/components/api/*.vue ./src/main/webapp/vue-app/components/todo-list/*.vue ./src/main/webapp/vue-app/components/*.vue",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "build": "webpack --config webpack.prod.js --mode production",
    "ganache-cli": "ganache-cli --account=0xc87509a1c067bbde78beb793e6fa76530b6382a4c0241e5e4a9ec0a0f44dc0d3,100000000000000000000 --networkId 4452364 --debug --db target/ganache-data"
  },
  "author": "eXo",
  "license": "LGPL",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.27",
    "abi-decoder": "^1.2.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "connect-api-mocker": "^1.4.0",
    "css-loader": "^0.28.11",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "ethereum-qr-code": "^0.3.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "flush-promises": "^1.0.2",
    "ganache-cli": "^6.4.3",
    "jest": "^22.4.4",
    "jest-fetch-mock": "^1.7.5",
    "jest-localstorage-mock": "^2.2.0",
    "jest-runner-eslint": "^0.5.0",
    "jest-vue-preprocessor": "^1.4.0",
    "jquery": "^3.3.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "npm": "^6.4.1",
    "prettier": "1.15.3",
    "vue": "^2.5.16",
    "vue-i18n": "^7.8.1",
    "vue-jest": "^3.0.1",
    "vue-loader": "^14.2.3",
    "vue-template-compiler": "^2.5.16",
    "web3": "^1.0.0-beta.36",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {},
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "<rootDir>/src/main/webapp/vue-app/*.js",
      "<rootDir>/src/main/webapp/vue-app/**/*.js",
      "<rootDir>/src/main/webapp/vue-app/**/*.vue"
    ],
    "projects": [
      {
        "displayName": "test",
        "testMatch": [
          "<rootDir>/src/test/specs/*.js"
        ],
        "moduleNameMapper": {
          "^vue$": "vue/dist/vue.common.js"
        },
        "moduleFileExtensions": [
          "js",
          "jsx",
          "ts",
          "tsx",
          "json",
          "node",
          "vue"
        ],
        "modulePaths": [
          "node_modules",
          "src"
        ],
        "transform": {
          "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
          ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
        },
        "roots": [
          "src"
        ],
        "setupFiles": [
          "<rootDir>/src/test/globals.js",
          "jest-localstorage-mock"
        ],
        "testURL": "http://localhost/"
      },
      {
        "displayName": "lint",
        "runner": "jest-runner-eslint",
        "testMatch": [
          "<rootDir>/src/test/specs/**/*.js"
        ]
      }
    ],
    "globalSetup": "<rootDir>/src/test/globalSetup.js",
    "globalTeardown": "<rootDir>/src/test/globalTeardown.js",
    "coverageDirectory": "<rootDir>/target/jest/coverage"
  }
}
