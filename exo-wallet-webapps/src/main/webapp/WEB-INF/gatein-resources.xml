  <module>
    <name>vuetify</name>
    <script>
      <minify>false</minify>
      <path>/js/lib/vuetify.min.js</path>
    </script>
    <depends>
      <module>vue</module>
    </depends>
  </module>

  <module>
    <name>walletCommon</name>
    <script>
      <minify>false</minify>
      <adapter>
        (function() {
          <include>/js/walletCommon.bundle.js</include>
          return WalletCommon;
        })();
      </adapter>
    </script>
    <depends>
      <module>vue</module>
    </depends>
    <depends>
      <module>jquery</module>
      <as>$</as>
    </depends>
    <depends>
      <module>walletJQueryUI</module>
    </depends>
    <depends>
      <module>fetchPolyfill</module>
    </depends>
    <depends>
      <module>customEventPolyfill</module>
    </depends>
    <depends>
      <module>iePolyfills</module>
    </depends>
    <depends>
      <module>vuetify</module>
    </depends>
    <depends>
      <module>Web3</module>
      <as>LocalWeb3</as>
    </depends>
    <depends>
      <module>ethQRCode</module>
      <as>EthereumQRPlugin</as>
