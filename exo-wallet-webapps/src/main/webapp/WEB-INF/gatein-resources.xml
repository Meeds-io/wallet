<gatein-resources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.exoplatform.org/xml/ns/gatein_resources_1_4 http://www.exoplatform.org/xml/ns/gatein_resources_1_4" xmlns="http://www.exoplatform.org/xml/ns/gatein_resources_1_4">
<!-- 
  We can't use this due to:
  https://jira.exoplatform.org/browse/PLF-8100

  <portlet-skin>
    <application-name>exo-ethereum-wallet</application-name>
    <portlet-name>EthereumWallet</portlet-name>
    <skin-name>Enterprise</skin-name>
    <css-path>/css/wallet.css</css-path>
  </portlet-skin>

  <portlet-skin>
    <application-name>exo-ethereum-wallet</application-name>
    <portlet-name>EthereumWallet</portlet-name>
    <skin-name>Default</skin-name>
    <css-path>/css/wallet.css</css-path>
  </portlet-skin>

  <portlet-skin>
    <application-name>exo-ethereum-wallet</application-name>
    <portlet-name>EthereumSpaceWallet</portlet-name>
    <skin-name>Enterprise</skin-name>
    <css-path>/css/wallet.css</css-path>
  </portlet-skin>

  <portlet-skin>
    <application-name>exo-ethereum-wallet</application-name>
    <portlet-name>EthereumSpaceWallet</portlet-name>
    <skin-name>Default</skin-name>
    <css-path>/css/wallet.css</css-path>
  </portlet-skin>

-->

  <portal-skin>
    <skin-name>Default</skin-name>
    <skin-module>wallet</skin-module>
    <css-path>/css/platform.css</css-path>
    <css-priority>0</css-priority>
  </portal-skin>

  <portal-skin>
    <skin-name>Enterprise</skin-name>
    <skin-module>wallet</skin-module>
    <css-path>/css/platform.css</css-path>
    <css-priority>0</css-priority>
  </portal-skin>

  <portlet>
    <name>EthereumWalletLink</name>
    <module>
      <script>
        <minify>false</minify>
        <path>/js/walletLink.js</path>
      </script>
      <depends>
        <module>jquery</module>
        <as>$</as>
      </depends>
    </module>
  </portlet>

  <portlet>
    <name>EthereumWalletAPI</name>
    <module>
      <script>
        <minify>false</minify>
        <path>/js/walletAPI.bundle.js</path>
      </script>
      <depends>
        <module>jquery</module>
        <as>$</as>
      </depends>
      <depends>
        <module>walletJQueryUI</module>
      </depends>
      <depends>
        <module>fetchPolyfill</module>
      </depends>
      <depends>
        <module>customEventPolyfill</module>
      </depends>
      <depends>
        <module>iePolyfills</module>
      </depends>
      <depends>
        <module>vuetify</module>
      </depends>
      <depends>
        <module>Web3</module>
        <as>LocalWeb3</as>
      </depends>
      <depends>
        <module>ethQRCode</module>
        <as>EthereumQRPlugin</as>
      </depends>
      <depends>
        <module>abiDecoder</module>
      </depends>
      <depends>
        <module>userPopupPlugin</module>
      </depends>
      <depends>
        <module>spacePopupPlugin</module>
      </depends>
    </module>
  </portlet>

  <portlet>
    <name>EthereumWallet</name>
    <module>
      <script>
        <minify>false</minify>
