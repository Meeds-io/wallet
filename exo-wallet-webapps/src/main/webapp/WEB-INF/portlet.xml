<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
             version="2.0"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
   <portlet>
     <portlet-name>EthereumWalletAPI</portlet-name>
     <display-name xml:lang="EN">Ethereum Wallet API Application</display-name>
     <portlet-class>org.exoplatform.commons.api.portlet.GenericDispatchedViewPortlet</portlet-class>
     <init-param>
      <name>portlet-view-dispatched-file-path</name>
      <value>/walletAPI.html</value>
     </init-param>
     <expiration-cache>-1</expiration-cache>
     <cache-scope>PUBLIC</cache-scope>
     <supports>
       <mime-type>text/html</mime-type>
     </supports>
     <supported-locale>en</supported-locale>
     <portlet-info>
       <title>Ethereum Wallet API Application</title>
       <keywords>Ethereum Wallet API Application</keywords>
     </portlet-info>
   </portlet>
   <portlet>
     <portlet-name>EthereumWalletLink</portlet-name>
     <display-name xml:lang="EN">Ethereum Wallet Tip Tip Link Application</display-name>
     <portlet-class>org.exoplatform.commons.api.portlet.GenericDispatchedViewPortlet</portlet-class>
     <init-param>
      <name>portlet-view-dispatched-file-path</name>
      <value>/walletLink.html</value>
     </init-param>
     <expiration-cache>-1</expiration-cache>
     <cache-scope>PUBLIC</cache-scope>
     <supports>
       <mime-type>text/html</mime-type>
     </supports>
     <supported-locale>en</supported-locale>
     <portlet-info>
       <title>Ethereum Wallet Tip Tip Link Application</title>
       <keywords>Ethereum Wallet Tip Tip Application</keywords>
     </portlet-info>
   </portlet>
   <portlet>
     <portlet-name>EthereumWallet</portlet-name>
     <display-name xml:lang="EN">Ethereum Wallet Application</display-name>
     <portlet-class>org.exoplatform.commons.api.portlet.GenericDispatchedViewPortlet</portlet-class>
     <init-param>
      <name>portlet-view-dispatched-file-path</name>
      <value>/wallet.jsp</value>
     </init-param>
     <expiration-cache>-1</expiration-cache>
     <cache-scope>PUBLIC</cache-scope>
     <supports>
       <mime-type>text/html</mime-type>
     </supports>
     <supported-locale>en</supported-locale>
     <portlet-info>
       <title>Ethereum Wallet Application</title>
       <keywords>Ethereum Wallet Application</keywords>
     </portlet-info>
   </portlet>
   <portlet>
     <portlet-name>EthereumSpaceWallet</portlet-name>
     <display-name xml:lang="EN">Ethereum Space Wallet Application</display-name>
     <portlet-class>org.exoplatform.commons.api.portlet.GenericDispatchedViewPortlet</portlet-class>
     <init-param>
      <name>portlet-view-dispatched-file-path</name>
      <value>/spaceWallet.html</value>
     </init-param>
     <expiration-cache>-1</expiration-cache>
     <cache-scope>PUBLIC</cache-scope>
     <supports>
       <mime-type>text/html</mime-type>
     </supports>
     <supported-locale>en</supported-locale>
     <portlet-info>
       <title>Ethereum Space Wallet Application</title>
       <keywords>Ethereum Space Wallet Application</keywords>
     </portlet-info>
   </portlet>
   <portlet>
     <portlet-name>RewardAdmin</portlet-name>
     <display-name xml:lang="EN">Ethereum Wallet Application Administration</display-name>
     <portlet-class>org.exoplatform.commons.api.portlet.GenericDispatchedViewPortlet</portlet-class>
     <init-param>
      <name>portlet-view-dispatched-file-path</name>
      <value>/rewardAdmin.html</value>
     </init-param>
     <expiration-cache>-1</expiration-cache>
     <cache-scope>PUBLIC</cache-scope>
     <supports>
       <mime-type>text/html</mime-type>
     </supports>
     <supported-locale>en</supported-locale>
     <portlet-info>
       <title>Ethereum Wallet Reward Administration</title>
       <keywords>Ethereum Wallet Reward Administration</keywords>
     </portlet-info>
      <security-role-ref>
        <role-name>admin</role-name>
        <role-link>admin</role-link>
      </security-role-ref>
   </portlet>
</portlet-app>